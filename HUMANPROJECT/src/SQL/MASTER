CREATE TABLE MASTER(
	P_SEQ NUMBER NOT NULL,
	E_IDENNUM NUMBER NOT NULL,
	P_START VARCHAR2(10) NOT NULL,
	P_END VARCHAR2(10) NULL,
	P_POSITION VARCHAR2(100) NOT NULL
)

DROP TABLE MASTER

ALTER TABLE MASTER
ADD CONSTRAINT FK_P_SEQ FOREIGN KEY(P_SEQ) REFERENCES PROJECT(P_SEQ)

ALTER TABLE MASTER
ADD CONSTRAINT FK_E_IDENNUM FOREIGN KEY(E_IDENNUM) REFERENCES EMPLOYEE(E_IDENNUM)