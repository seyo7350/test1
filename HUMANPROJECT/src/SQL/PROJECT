CREATE TABLE PROJECT(
	P_SEQ NUMBER NOT NULL PRIMARY KEY,
	P_NAME VARCHAR2(300) NOT NULL,
	P_CONT VARCHAR2(500) NOT NULL,
	P_CNT NUMBER NULL,
	P_SKILL VARCHAR2(500) NOT NULL,
	P_ORDER VARCHAR2(100) NOT NULL
)

DROP TABLE PROJECT

create sequence P_SEQ
increment by 1
start with 1
maxvalue 99999999