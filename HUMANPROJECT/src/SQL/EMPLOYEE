CREATE TABLE EMPLOYEE(
	E_SEQ NUMBER NOT NULL,
	E_IDENNUM NUMBER NOT NULL PRIMARY KEY,
	E_NAME VARCHAR2(10) NOT NULL,
	E_SALRAY VARCHAR2(10) NOT NULL,
	E_RANK NUMBER NOT NULL,
	E_PART NUMBER NOT NULL,
	E_POST VARCHAR2(10) NOT NULL,
	E_ADD VARCHAR2(50) NOT NULL,
	E_DADD VARCHAR2(50) NOT NULL,
	E_EMAIL VARCHAR2(50) NOT NULL,
	E_IDEN VARCHAR2(20) NOT NULL,
	E_PHONE VARCHAR2(20) NOT NULL,
	E_PASS VARCHAR2(20) NOT NULL,
	E_IMAGE VARCHAR2(1000) NOT NULL,
	E_OUT NUMBER NOT NULL
)

DROP TABLE EMPLOYEE

create sequence E_SEQ
increment by 1
start with 1
maxvalue 99999999

